<editors>
    <!-- Boolean -->
    <editor id="bool">
        <range uom="2" subset="0,1" />
    </editor>
    
    <editor id="door_status">
        <range uom="25" subset="0-6" nls="STATUS" />
    </editor>
    
    <editor id="doors">
        <range uom="25" subset="0-1" nls="DOORS" />
    </editor>
    
    <editor id="num_doors">
        <range uom="56" min="1" max="2" prec="0"/>
    </editor>
    
    <editor id="pi_temp_c">
        <range uom="4" min="-50" max="100" step="1" prec="0"/>
    </editor>
    
</editors>
